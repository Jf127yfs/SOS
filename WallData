    const uids = musicGroups[music];
    connections[music] = [];
    
    for (let i = 0; i < uids.length; i++) {
      for (let j = i + 1; j < uids.length; j++) {
        connections[music].push({
          source: uids[i],
          target: uids[j]
        });
      }
    }
  });
  
  return connections;
}

